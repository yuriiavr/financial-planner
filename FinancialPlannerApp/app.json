{
  "expo": {
    "name": "FinancialPlannerApp",
    "slug": "financial-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "myapp",
    "ios": {
      "bundleIdentifier": "com.mycompany.financialplannerapp",
      "supportsTablet": true
    },
    "android": {
      "package": "com.mycompany.financialplannerapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playstore.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/appstore.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "27465896-e388-4773-82ab-bdb2e8404ef6"
      }
    }
  }
}
